{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/main",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}